
<h1 class="main-title">Hola Anna...</h1>

<% @timelines.each do |timeline| %>

<div class="entrie">
  <div class="entrie-head">
    <div class="time">Creado el <%= timeline.created_at.in_time_zone('Lima').strftime("%d-%m-%Y a las %H:%M") %></div>
    <div class="title"><%= timeline.title %></div>
  </div>
  <div class="entrie-content"><%= timeline.content %></div>
</div>

<% end %>

<br>
